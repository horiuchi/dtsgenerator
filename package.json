{
  "name": "dtsgenerator",
  "version": "0.8.2",
  "description": "TypeScript d.ts file generator for JSON Schema file",
  "main": "index.js",
  "bin": {
    "dtsgen": "./bin/dtsgen"
  },
  "scripts": {
    "clean": "rimraf src/**/*.js* && rimraf test/**/*.js*",
    "prebuild": "tslint --project .",
    "build": "tsc -p . && babel src --out-dir src",
    "watch": "tsc -p . -w --noEmit",
    "test": "nyc --extension .ts mocha --compilers ts:ts-node/register,babel-register",
    "report-coverage": "nyc report --reporter=lcov && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:nocov": "mocha --compilers ts:ts-node/register,babel-register"
  },
  "directories": {
    "test": "test/"
  },
  "keywords": [
    "TypeScript",
    "JSON Schema"
  ],
  "author": "horiuchi",
  "license": "MIT",
  "homepage": "https://github.com/horiuchi/dtsgenerator",
  "repository": {
    "type": "git",
    "url": "git@github.com:horiuchi/dtsgenerator.git"
  },
  "bugs": {
    "url": "https://github.com/horiuchi/dtsgenerator/issues"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "glob": "^7.1.1",
    "mkdirp": "^0.5.1",
    "request": "^2.78.0"
  },
  "devDependencies": {
    "@types/commander": "^2.3.30",
    "@types/debug": "0.0.29",
    "@types/glob": "^5.0.30",
    "@types/mkdirp": "^0.3.29",
    "@types/mocha": "^2.2.32",
    "@types/power-assert": "^1.4.29",
    "@types/request": "0.0.32",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.14",
    "mocha": "^3.1.2",
    "nyc": "^8.4.0",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.6",
    "ts-node": "^1.7.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.7"
  }
}
